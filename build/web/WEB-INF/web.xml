<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>LoginCheck</servlet-name>
        <servlet-class>controller.LoginCheck</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegisterServlet</servlet-name>
        <servlet-class>controller.RegisterServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SignOut</servlet-name>
        <servlet-class>controller.SignOut</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>searchShow</servlet-name>
        <servlet-class>controller.searchShow</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>profileServlet</servlet-name>
        <servlet-class>controller.profileServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProfileServlet1</servlet-name>
        <servlet-class>controller.ProfileServlet1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProfileSaveServlet</servlet-name>
        <servlet-class>controller.ProfileSaveServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginCheck</servlet-name>
        <url-pattern>/LoginCheck</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegisterServlet</servlet-name>
        <url-pattern>/RegisterServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SignOut</servlet-name>
        <url-pattern>/SignOut</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>searchShow</servlet-name>
        <url-pattern>/searchShow</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>profileServlet</servlet-name>
        <url-pattern>/profileServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProfileServlet1</servlet-name>
        <url-pattern>/ProfileServlet1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProfileSaveServlet</servlet-name>
        <url-pattern>/ProfileSaveServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
